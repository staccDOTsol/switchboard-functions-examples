{
  "name": "@switchboard-xyz/task-runner",
  "version": "2.6.2",
  "description": "Switchboard task runner module",
  "repository": {
    "type": "git",
    "url": "https://github.com/switchboard-xyz/sbv3.git",
    "directory": "javascript/task-runner"
  },
  "license": "MIT",
  "author": "mitch@switchboard.xyz",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./runner": {
      "types": "./runner.d.ts",
      "import": "./runner.js",
      "require": "./runner.cjs"
    },
    "./simulator": {
      "types": "./simulator.d.ts",
      "import": "./simulator.js",
      "require": "./simulator.cjs"
    },
    "./utils": {
      "types": "./utils.d.ts",
      "import": "./utils.js",
      "require": "./utils.cjs"
    },
    "./clients": {
      "types": "./clients.d.ts",
      "import": "./clients.js",
      "require": "./clients.cjs"
    },
    "./tasks": {
      "types": "./tasks.d.ts",
      "import": "./tasks.js",
      "require": "./tasks.cjs"
    },
    "./ctx": {
      "types": "./ctx.d.ts",
      "import": "./ctx.js",
      "require": "./ctx.cjs"
    },
    "./errors": {
      "types": "./errors.d.ts",
      "import": "./errors.js",
      "require": "./errors.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "lib/",
    "package.json",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "runner.cjs",
    "runner.js",
    "runner.d.ts",
    "simulator.cjs",
    "simulator.js",
    "simulator.d.ts",
    "utils.cjs",
    "utils.js",
    "utils.d.ts",
    "clients.cjs",
    "clients.js",
    "clients.d.ts",
    "tasks.cjs",
    "tasks.js",
    "tasks.d.ts",
    "ctx.cjs",
    "ctx.js",
    "ctx.d.ts",
    "errors.cjs",
    "errors.js",
    "errors.d.ts"
  ],
  "scripts": {
    "start": "node lib/index.js",
    "watch:workers": "node ./esbuild.js -watch",
    "watch:tsc": "tsc -p tsconfig.cjs.json --watch",
    "watch": "run-p watch:*",
    "test": "jest --maxWorkers=5 --testTimeout 30000 --silent=false --verbose",
    "test:ci": "jest --ci --runInBand --testTimeout 30000 --silent=false --verbose",
    "test:coverage": "jest --coverage --testTimeout 30000",
    "build": "node esbuild.js",
    "build:bundle": "node esbuild.bundle.js",
    "lint": "pnpm exec eslint src/**/*.ts",
    "clean": "pnpm exec rimraf node_modules lib .turbo",
    "fix": "pnpm exec eslint --fix src/**/*.ts",
    "run:publish": "pnpm build:bundle && cd dist && pnpm install && pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@blockworks-foundation/mango-client": "^3.6.20",
