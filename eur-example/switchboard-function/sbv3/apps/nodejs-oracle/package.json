{
  "name": "nodejs-oracle",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "tsup": {
    "entry": [
      "src/apps/oracle/index.ts",
      "src/apps/crank/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": true,
    "dts": true
  },
  "scripts": {
    "build": "node ./esbuild.js",
    "docker:build:oracle:local": "docker buildx build -f ./Dockerfile -t switchboardlabs/node:dev-\"$(cat ../../version)\" --pull --load ./",
    "docker:build:oracle": "docker buildx build -f ./Dockerfile -t switchboardlabs/node:dev-\"$(cat ../../version)\" --platform linux/amd64 --pull --load ./",
    "docker:publish:oracle": "docker buildx build -f ./Dockerfile -t switchboardlabs/node:dev-\"$(cat ../../version)\" --platform linux/amd64 --pull --push ./",
    "docker:build:crank": "docker buildx build -f ./Dockerfile.crank -t switchboardlabs/crank-turn:dev-\"$(cat ../../version)\" --platform linux/amd64 --pull --load ./",
    "docker:publish:crank": "docker buildx build -f ./Dockerfile.crank -t switchboardlabs/crank-turn:dev-\"$(cat ../../version)\" --platform linux/amd64 --pull --push ./",
    "docker:build": "run-p docker:build:*",
    "docker:publish": "run-p docker:publish:*",
    "test": "pnpm exec jest",
    "lint": "pnpm exec eslint src",
    "clean": "pnpm exec rimraf node_modules lib .turbo",
    "fix": "pnpm exec eslint --fix src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@coral-xyz/anchor": "^0.28.0",
    "@ethersproject/providers": "^5.7.2",
    "@isaacs/ttlcache": "^1.4.0",
    "@mysten/sui.js": "0.32.2",
    "@opentelemetry/api": "1.1",
    "@opentelemetry/api-metrics": "^0.27.0",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-prometheus": "^0.27.0",
    "@opentelemetry/host-metrics": "^0.27.1",
    "@opentelemetry/sdk-metrics-base": "^0.27.0",
    "@pagerduty/pdjs": "^2.2.4",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.87.6",
    "@switchboard-xyz/aptos.js": "workspace:^",
    "@switchboard-xyz/common": "workspace:^",
    "@switchboard-xyz/ecvrf-wasm": "^1.0.3",
    "@switchboard-xyz/evm.js": "=3.9.4",
    "@switchboard-xyz/near.js": "workspace:^",
    "@switchboard-xyz/node": "workspace:^",
    "@switchboard-xyz/solana.js": "workspace:^",
    "@switchboard-xyz/sui.js": "workspace:^",
    "@switchboard-xyz/task-runner": "workspace:^",
    "aptos": "^1.11.0",
    "axios": "^1.2.0",
    "bs58": "^5.0.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "lru-cache": "^7.10.1",
    "near-api-js": "^2.0.4",
    "undici": "^5.22.0",
    "wait-for-event": "^2.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
