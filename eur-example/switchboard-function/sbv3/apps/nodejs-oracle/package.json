    "@types/node": "^18.16.18",
    "esbuild": "^0.17.16",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "npm-dts": "^1.3.12",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.0.0"
  }
