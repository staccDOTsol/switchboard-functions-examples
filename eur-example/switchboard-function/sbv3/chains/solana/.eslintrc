{
  "extends": "@switchboard-xyz",
  "root": true,
  "rules": {
    "quotes": "off",
    "@typescript-eslint/ban-ts-comment": "off",
    "no-control-regex": "off",
    "node/no-unpublished-import": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}