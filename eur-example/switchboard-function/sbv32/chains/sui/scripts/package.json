{
  "name": "switchboard-sui-scripts",
  "description": "Switchboard SUI Scripts",
  "version": "0.0.1",
  "license": "MIT",
  "files": [
    "scripts",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "shx rm -rf lib || true; tsc -p tsconfig.cjs.json && tsc",
    "watch": "tsc -p tsconfig.cjs.json --watch",
    "test": "ts-mocha -p ./tsconfig.cjs.json --require ts-node/register -t 1000000 ./tests/*.tests.ts",
    "prepack": "npm run lint && npm run build",
    "lint": "gts lint ./src",
    "fix": "gts fix ./src",
    "clean": "gts clean"
  },
  "dependencies": {
    "@mysten/sui.js": "0.32.2",
    "@switchboard-xyz/common": "^2.3.15",
    "@switchboard-xyz/sui.js": "^2.3.14",
    "big.js": "^6.2.1",
    "bn.js": "^5.2.1",
    "chalk": "^5.0.1",
    "js-sha3": "^0.8.0",
    "node-fetch": "3.3.0"
  },
  "devDependencies": {
    "@switchboard-xyz/eslint-config": "latest",
    "@types/big.js": "^6.1.6",
    "@types/bn.js": "^5.1.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.13.0",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.11",
    "eslint": "^8.35.0",
    "gts": "^3.1.1",
    "mocha": "^10.1.0",
    "shx": "^0.3.4",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  }
}
