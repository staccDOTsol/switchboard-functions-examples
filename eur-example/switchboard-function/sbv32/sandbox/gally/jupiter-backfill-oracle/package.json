{
  "name": "backfill-oracle-program",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "docker:build": "docker buildx build -f Dockerfile --platform linux/amd64 --tag switchboardlabs/backfill-oracle-worker:dev-$(cat ../../../version) --pull .",
    "docker:publish": "echo \"Building switchboardlabs/backfill-oracle-worker:dev-$(cat ../../../version) ...\"; docker buildx build -f Dockerfile --platform linux/amd64 --tag switchboardlabs/backfill-oracle-worker:dev-$(cat ../../../version) --pull --push . && echo \"Published switchboardlabs/backfill-oracle-worker:dev-$(cat ../../../version)\"",
    "fix": "cargo fmt",
    "test": "cargo test",
    "prod:set": "az aks get-credentials -g Default -n sb-internal",
    "prod:uprev": "helm upgrade -i backfill-oracle-worker ./helm-chart -f helm-values.yaml",
    "prod:restart": "kubectl delete deployment backfill-oracle-worker && sleep 10 && pnpm prod:uprev",
    "prod:logs": "kubectl logs -f -l app=backfill-oracle-worker --all-containers --max-log-requests=100"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@switchboard-xyz/solana.js": "*",
    "big.js": "^6.2.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "chalk": "4",
    "dotenv": "^16.3.1",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "tsx": "^4.1.1",
    "typescript": "^4.3.5"
  }
}
